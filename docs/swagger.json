        },
        "version": "1.0"
    },
    "host": "temp.swagger.io",
    "basePath": "/",
    "paths": {
        "/album/{id}": {
            "get": {
                "description": "get a album by its id",
                "produces": [
                    "application/json"
                ],
                "operationId": "get album by id",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Album ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {}
            }
        },
        "/albums": {
            "get": {
                "description": "get all albums",
                "produces": [
                    "application/json"
                ],
                "operationId": "get albums",
                "responses": {}
            }
        },
        "/health": {
            "get": {
                "description": "get healthcheck",
                "produces": [
                    "application/json"
                ],
                "summary": "Check the health of the server",
                "operationId": "health",
                "responses": {}
            }
        }
    }
}